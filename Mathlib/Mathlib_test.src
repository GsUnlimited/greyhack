import_code("../TestFramework/TestFramework.src":"../TestFramework/TestFramework.src")
import_code("Mathlib.src":"Mathlib.src")
TestMathlib = new TestCase.construct("TestMathlib")
TestMathlib.assertSame(pow(2,1),2,"test that pow(2,1) is same as 2")
TestMathlib.assertSame(pow(2,2),4,"test that pow(2,2) is same as 4")
TestMathlib.assertSame(pow(2,3),8,"test that pow(2,3) is same as 8")
TestMathlib.assertSame(pow(2,4),16,"test that pow(2,4) is same as 16")
TestMathlib.assertSame(pow(0,1),0,"test that pow(0,1) is same as 0")
TestMathlib.assertSame(pow(5,3),125,"test that pow(5,3) is same as 125")
TestMathlib.assertSame(pow(16,3),4096,"test that pow(16,3) is same as 4096")
TestMathlib.assertNotSame(pow(16,2),4096,"test that pow(16,3) is not same as 4096")